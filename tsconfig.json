{
  "compilerOptions": {
    "target": "es2019",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "alwaysStrict": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "incremental": true,
    "baseUrl": "./",
    "lib": [
      "es2019",
      "dom"
    ]
  }
}
